<!DOCTYPE html>
<html>
  <head>
    <title>GuestAdvisor</title>
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon"></link>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
    <script src ="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <script src="/javascripts/application.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css"></link>
    <link href='https://fonts.googleapis.com/css?family=Amatic+SC:700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=PT+Sans+Narrow' rel='stylesheet' type='text/css'>
    <link rel='stylesheet' href='/stylesheets/style.css'></link>

  </head>
  <body>
    <div class="row">
      <nav class="navbar navbar-inverse header">
        <div class="row container-fluid">
          <div class="navbar-header col-md-2 col-sm-2 col-xs-2">
            <a style="margin-left: 2%; color: #EFCA08" class="logo navbar-brand" href="/users">GuestAdvisor</a>
          </div>
          <div class="navbar-brand col-md-2 col-sm-2 col-xs-2">
            <% if (user) { %>
              <p style="color: #EFCA08; font-family: 'PT Sans Narrow', sans-serif">Hello <%= user.name %>!</p>
            <% } else { %>
              <p style="color: #EFCA08">Please Login</p>
            <% } %>
          </div>
          <div class="col-md-4 col-sm-4 col-xs-4">
            <form class="navbar-form navbar-left" role="search">
              <div class="form-group">
                <input type="text" id ="search-term" class="form-control" placeholder="Search">
              </div>
              <button type="submit" id ="submitit" class="btn btn-default">Submit</button>
            </form> 
          </div>
          <div class="col-md-4 col-sm-4 col-xs-4">
            <ul class="nav navbar-nav navbar-right">
              <% if (user) { %>
              <li><a href='/users' style="color: #EFCA08"><span class="glyphicon glyphicon-home"></span> Home</a></li>
              <li><a href="/logout" style="color: #EFCA08"><span class="glyphicon glyphicon-log-out"></span> Logout</a></li>
              <% } else { %>
                <li><a href="/login" style="color: #EFCA08"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
                <li><a href="/new_user" style="color: #EFCA08"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
              <% } %>
            </ul>
          </div>
        </div>
      </nav>
    </div>
    <div class="row">

      <!-- LIVE FEED FILLER CONTENT -->
      <div class="col-md-3 col-sm-3 col-xs-3">
        <div class="panel panel-default" id="live_feed_panel">
          <div>
            <h2 style="text-align: center" class="panel-title">Latest Review</h2>
          </div>
          <div class="panel-body">
            <ul style="text-align: center; list-style: none">
              <div id="live">
              </div> 
             </ul> 
          </div>
        </div>
      </div>
      <div class="col-md-1 col-sm-1 col-xs-1">
      </div>

