<% include ./partials/header %>

      <form action="#" method="post" enctype="multipart/form-data">
         <fieldset>
            <label for="name">Name:</label>
            <input type="text" id="name" name="name" placeholder="Enter guest's name" />
            <br />
            <label for="email">Email:</label>
            <input type="email" id="email" name="email" placeholder="Enter guest's email" />
            <br />
            <label for="review">Review:</label>
            <textarea id="description" name="description" placeholder="Enter your guest review here"></textarea>
            <br />
            <input type="submit" value="Submit New Review" />
         </fieldset>
      </form>